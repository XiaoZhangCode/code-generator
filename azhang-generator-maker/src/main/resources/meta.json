{
  "name": "acm-template-pro-generator",
  "description": "ACM 示例模板生成器",
  "basePackage": "com.azhang",
  "version": "1.0",
  "author": "azhang",
  "createTime": "2024-1-18",
  "forcedInteractiveSwitch": true,
  "versionControl": true,
  "fileConfig": {
    "inputRootPath": ".source/acm-template-pro",
    "sourceRootPath": "D:/Program Files/code/me/zhangPro/azhang-generator/azhang-generator-demo-projects/acm-template-pro",
    "outputRootPath": "generated",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/com/azhang/acm/MainTemplate.java.ftl",
        "outputPath": "src/com/azhang/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      },
      {
        "groupKey": "acmTemplate",
        "groupName": "gitignore 和 README.md",
        "type": "group",
        "condition": "needGit",
        "files": [
          {
            "inputPath": ".gitignore",
            "outputPath": ".gitignore",
            "type": "file",
            "generateType": "static",
            "condition": "needGit"
          },
          {
            "inputPath": "README.md",
            "outputPath": "README.md",
            "type": "file",
            "generateType": "static"
          }
        ]
      }
    ]
  },
  "modelConfig": {
    "models": {
      "dataModel": {
        "description": "动态模板配置",
        "filedInfo": [
          {
            "fieldName": "needGit",
            "type": "Boolean",
            "description": "是否生成.gitignore文件",
            "defaultValue": true,
            "abbr": "n"
          },
          {
            "fieldName": "loop",
            "type": "Boolean",
            "description": "是否生成循环",
            "defaultValue": false,
            "abbr": "l"
          },
          {
            "groupKey": "mainTemplate",
            "groupName": "核心模板",
            "type": "MainTemplate",
            "description": "核心模板参数",
            "condition": "loop",
            "models": [
              {
                "fieldName": "author",
                "type": "String",
                "description": "作者注释",
                "defaultValue": "azhang",
                "abbr": "a"
              },
              {
                "fieldName": "outputText",
                "type": "String",
                "description": "输出信息",
                "defaultValue": "sum = ",
                "abbr": "o"
              }
            ]
          }
        ]
      }
    }
  }
}
