{
  "name": "acm-template-pro-generator",
  "description": "ACM 示例模板生成器",
  "basePackage": "com.azhang",
  "version": "1.0",
  "author": "azhang",
  "createTime": "2024-1-18",
  "forcedInteractiveSwitch": true,
  "fileConfig": {
    "inputRootPath": "D:/Program Files/code/me/zhangPro/azhang-generator/azhang-generator-demo-projects/acm-template-pro",
    "outputRootPath": "generated",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/com/azhang/acm/MainTemplate.java.ftl",
        "outputPath": "src/com/azhang/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      },
      {
        "inputPath": ".gitignore",
        "outputPath": ".gitignore",
        "type": "file",
        "generateType": "static"
      },
      {
        "inputPath": "README.md",
        "outputPath": "README.md",
        "type": "file",
        "generateType": "static"
      }
    ]
  },
  "modelConfig": {
    "models": {
      "dataModel":{
        "description": "动态模板配置",
        "filedInfo": [
          {
            "fieldName": "loop",
            "type": "Boolean",
            "description": "是否生成循环",
            "defaultValue": false,
            "abbr": "l"
          },
          {
            "fieldName": "author",
            "type": "String",
            "description": "作者注释",
            "defaultValue": "azhang",
            "abbr": "a"
          },
          {
            "fieldName": "outputText",
            "type": "String",
            "description": "输出信息",
            "defaultValue": "sum = ",
            "abbr": "o"
          }
        ]
      }
    }
  }
}
